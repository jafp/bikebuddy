<!doctype html>
<html lang="en" ng-app="bb">
<head>
  <meta charset="utf-8">
  <title>bikebuddy - find en cykeltur</title>
  <link rel="icon" type="image/png" href="/static/img/bike-icon.png" />
  <link href='https://fonts.googleapis.com/css?family=Ubuntu' rel='stylesheet' type='text/css'>
  <!--<link rel="stylesheet" href="/static/build/bikebuddy.min.css"/>-->
  <link rel="stylesheet" href="/static/lib/jquery-ui/css/smoothness/jquery-ui.css" />
  <link rel="stylesheet" href="/static/lib/bootstrap/css/bootstrap.css" />
  <link rel="stylesheet" href="/static/css/app.css" />
</head>
<body ng-controller="AppCtrl">

<div class="outer">
	<div class="header">
		<div class="grid">
			<div class="col4">
				<h1><a href="#/"><img src="/static/img/bike-medium.png"/> BikeBuddy</a></h1>
			</div>
			<div class="col8">
				<div class="links">
					<a href="#/ture">Ture</a>
					<!--<a href="#/">Artikler</a>-->
					<a href="#/om">Om Bikebuddy</a>

					<span ng-show="user">
						<a href="#/profil"><i class="icon-white icon-user"></i> {{ user.name }}</a>
						<a href="" ng-click="logout()" class="logout"><small>(log ud)</small></a>
					</span>
					<span ng-show="!user">
						<a href="#/logind"><i class="icon-white icon-user"></i> Log ind</a>
					</span>
				</div>
			</div>
		</div>
	</div>
	<div class="main">
		<div ng-view>&nbsp;</div>
	</div>
</div>

<script src="/static/lib/jquery.js"></script>
<script src="/static/lib/jquery-ui/js/jquery-ui.min.js"></script>
<script src="/static/lib/jquery.iframe-post-form.js"></script>
<script src="/static/lib/underscore-min.js"></script>
<script src="/static/lib/moment.min.js"></script>
<script src="/static/lib/angular/angular.min.js"></script>
<script src="/static/lib/bootstrap/js/bootstrap.min.js"></script>
<!--<script src="/static/lib/datepicker/js/bootstrap-datepicker.js"></script>-->

<script src="/static/js/app.js"></script>
<script src="/static/js/controllers.js"></script>
<script src="/static/js/directives.js"></script>
<script src="/static/js/filters.js"></script>
<script src="/static/js/services.js"></script>

</body>
</html>
